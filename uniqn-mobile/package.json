{
  "name": "uniqn-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"app/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" \"app/**/*.{ts,tsx,js,jsx}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "build:web": "expo export -p web",
    "build:android": "expo prebuild --platform android",
    "build:ios": "expo prebuild --platform ios",
    "analyze:bundle": "expo export -p web && source-map-explorer dist/_expo/static/js/web/*.js --html bundle-report.html",
    "analyze:bundle:ci": "expo export -p web && node scripts/check-bundle-size.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "deploy:cloudflare": "node scripts/deploy-cloudflare.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-firebase/app": "^23.8.6",
    "@react-native-firebase/auth": "^23.8.6",
    "@sentry/react-native": "~7.2.0",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.90.12",
    "date-fns": "^4.1.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.10",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "firebase": "^12.6.0",
    "jsqr": "^1.4.0",
    "nativewind": "^4.2.1",
    "react": "19.1.4",
    "react-dom": "19.1.4",
    "react-hook-form": "^7.68.0",
    "react-native": "^0.81.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^4.1.2",
    "react-native-nitro-modules": "^0.33.9",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.19",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "babel-preset-expo": "^54.0.8",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.0",
    "react-refresh": "^0.18.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "react-native-safe-area-context": "~5.6.0",
    "react-test-renderer": "19.1.0"
  },
  "private": true
}
